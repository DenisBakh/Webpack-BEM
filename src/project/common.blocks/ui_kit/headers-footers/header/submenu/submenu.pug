include /project/common.blocks/ui_kit/form_elements/bullet-list/bullet-list.pug

mixin submenu(list)
	.submenu-header
		.submenu-header__body
			.submenu-header__row
				.submenu-header__column
					ul.submenu-header__list
						each item in list
							li.submenu-header__item
								a.submenu-header__link=item

